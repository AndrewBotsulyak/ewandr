{
  "name": "client_product_details",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/clients/client_product_details/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/clients/client_product_details/browser",
        "index": "apps/clients/client_product_details/src/index.html",
        "main": "apps/clients/client_product_details/src/main.ts",
        "polyfills": [],
        "tsConfig": "apps/clients/client_product_details/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/clients/client_product_details/public"
          }
        ],
        "styles": ["apps/clients/client_product_details/src/styles.scss"],
        "customWebpackConfig": {
          "path": "apps/clients/client_product_details/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/clients/client_product_details/webpack.prod.config.ts"
          },
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": true
            },
            "fonts": {
              "inline": true
            }
          },
          "buildOptimizer": true,
          "aot": true,
          "sourceMap": false,
          "extractLicenses": true,
          "namedChunks": false,
          "vendorChunk": false,
          "commonChunk": true,
          "subresourceIntegrity": true
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202"
      },
      "configurations": {
        "production": {
          "buildTarget": "client_product_details:build:production"
        },
        "development": {
          "buildTarget": "client_product_details:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "client_product_details:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/clients/client_product_details/jest.config.ts",
        "tsConfig": "apps/clients/client_product_details/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "client_product_details:build",
        "port": 4202,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "client_product_details:build:development"
        },
        "production": {
          "buildTarget": "client_product_details:build:production"
        }
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@nx/angular:webpack-server",
      "options": {
        "outputPath": "dist/apps/clients/client_product_details/server",
        "main": "apps/clients/client_product_details/src/server.ts",
        "tsConfig": "apps/clients/client_product_details/tsconfig.server.json",
        "inlineStyleLanguage": "scss",
        "customWebpackConfig": {
          "path": "apps/clients/client_product_details/webpack.server.config.ts"
        }
      },
      "configurations": {
        "production": {
          "outputHashing": "media",
          "optimization": true,
          "buildOptimizer": true,
          "sourceMap": false
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "client_product_details:build:development",
          "serverTarget": "client_product_details:server:development"
        },
        "production": {
          "browserTarget": "client_product_details:build:production",
          "serverTarget": "client_product_details:server:production"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202
      }
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "client_product_details:build:development",
          "serverTarget": "client_product_details:server:development"
        },
        "production": {
          "browserTarget": "client_product_details:build:production",
          "serverTarget": "client_product_details:server:production"
        }
      },
      "defaultConfiguration": "production"
    },
    "static-server": {
      "dependsOn": ["build", "server"],
      "executor": "nx:run-commands",
      "options": {
        "command": "PORT=4202 node dist/apps/clients/client_product_details/server/main.js"
      }
    }
  }
}
